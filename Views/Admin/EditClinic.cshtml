@model Clinic

<form asp-controller="Admin" asp-action="EditClinic" method="post">
    <input type="hidden" asp-for="ClinicId">
    <div class="mb-3">
        <label asp-for="ClinicName" class="form-label">Name</label>
        <input asp-for="ClinicName" class="form-control">
    </div>

   

    <button type="submit" class="btn btn-primary">Update</button>
</form>
