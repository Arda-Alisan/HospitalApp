@model IEnumerable<HospitalApp.Models.Clinic>

<h1>Delete Clinics</h1>

<ul>
    @foreach (var clinic in Model)
    {
        <li>
            <a asp-controller="Admin" asp-action="DeleteClinic" asp-route-id="@clinic.ClinicId">
                @clinic.ClinicName
            </a>
        </li>
    }
</ul>